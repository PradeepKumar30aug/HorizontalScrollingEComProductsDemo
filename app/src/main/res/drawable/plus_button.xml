<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <item android:state_pressed="false">        <shape android:shape="rectangle">            <solid android:color="@color/button_pressed_false"></solid>            <stroke android:color="@color/button_pressed_false"                android:width="2dp"/>            <corners android:radius="0dp"/>        </shape>    </item>    <item android:state_pressed="true">        <shape android:shape="rectangle">            <solid android:color="@color/button_pressed_true"></solid>            <stroke android:color="@color/button_pressed_true"                android:width="2dp"/>            <corners android:radius="0dp"/>        </shape>    </item></selector>